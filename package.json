{
  "name": "taskplanner",
  "version": "1.0.0",
  "description": "MERN stack student task planner",
  "main": "server2/index.js",
  "scripts": {
    "start": "node server2/index.js",
    "server": "nodemon server2/index.js",
    "client": "npm start --prefix an",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install --prefix an && npm run build --prefix an",
    "render-build": "npm install && npm install --prefix an && npm run build --prefix an"
  },
  "engines": {
    "node": "22.16.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "dotenv": "^8.2.0",
    "cors": "^2.8.5",
    "body-parser": "^1.19.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "concurrently": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnilaAnilaN/taskPlanner.git"
  },
  "author": "AnilaAnilaN",
  "license": "ISC"
}
